---
type link = {
	name: string,
	url:string
}

let navbar:link[] = [
	{
		name: 'Home',
		url:'#'
	},
    {
        name:'About',
        url:'#'
    },
    {
        name:'Services',
        url:'#'
    },
    {
        name:'Work',
        url:'#'
    },
    {
        name:'Contact',
        url:'#'
    }
]
---
<nav class="fixed top-0 z-10 text-white flex w-full justify-between px-20 p-8 ">
    <a id="logo" >logo</a>
    <ul class="flex gap-8 ">
        {
            navbar.map((nav)=>(
                <li><a class="hover:underline text-lg" href={nav.url}>{nav.name}</a></li>
            ))
        }
    </ul>
</nav>